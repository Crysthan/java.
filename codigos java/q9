import java.util.Scanner;


public class App {
    public static void main(String[] args) throws Exception {

        int notas = 0, soma = 0;
        int cont = 0;

        Scanner leia = new Scanner(System.in);

        while (cont < 3) {
            System.out.println("inf 3 notas");
            notas = leia.nextInt();
            soma += notas;
            cont++;
        }

        System.out.println(+soma / 3);

    }

}
